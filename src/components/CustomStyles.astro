---
import '@fontsource/inter/variable.css';

// Primary 40% lighter hsl(140,100%,81%) Hex: 9EFFBE
// Accent 40% darker hsl(207, 84%, 21%) Hex: 093A63
// Nunito
// Dosis
---

<style is:inline is:global>
  :root {
    --aw-font-sans: 'InterVariable';
    --aw-font-serif: var(--aw-font-sans);
    --aw-font-heading: var(--aw-font-sans);

    --aw-color-primary: hsl(140, 100%, 41%);
    --aw-color-secondary: hsl(156, 77%, 43%);
    --aw-color-accent: hsl(207, 84%, 61%);
    --aw-color-text-page: rgb(17 24 39);
    --aw-color-text-muted: rgb(75 85 99);
    --aw-color-bg-page: rgb(255 255 255);
  }
</style>
